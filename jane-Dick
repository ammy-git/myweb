pipeline{
    agent any
    
    environment{
        PATH = "/opt/maven/maven/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                 git 'https://github.com/ammy-git/myweb'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
            }
         
         }
        
        
      }
        
}
        
